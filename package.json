{
	"name": "page-translate-robot",
	"version": "1.0.0",
	"description": "夢語るだけのREADME。",
	"main": "index.js",
	"scripts": {
		"build:firefox": "webpack --mode=production --config=webpack.config.ts --env=browser=firefox",
		"browser:firefox": "web-ext run --source-dir=./dist --start-url about:debugging about:addons",
		"prebrowser:firefox-sk": "mkdirp X:\\fx",
		"browser:firefox-sk": "npm run browser:firefox -- --keep-profile-changes --firefox-profile=X:\\fx ",
		"lint:firefox": "web-ext lint --source-dir=./dist",
		"check": "ncu",
		"update": "ncu -u && npm install"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/sk-0520/page-translate-robot.git"
	},
	"author": "sk",
	"license": "WTFPL",
	"bugs": {
		"url": "https://github.com/sk-0520/page-translate-robot/issues"
	},
	"homepage": "https://github.com/sk-0520/page-translate-robot#readme",
	"devDependencies": {
		"@types/webpack": "^5.28.0",
		"cp-cli": "^2.0.0",
		"css-loader": "^6.7.1",
		"jest": "^29.1.2",
		"mkdirp": "^1.0.4",
		"normalize-sass": "^1.0.0",
		"npm-check-updates": "^16.3.11",
		"npm-run-all": "^4.1.5",
		"rimraf": "^3.0.2",
		"sass": "^1.55.0",
		"sass-loader": "^13.1.0",
		"ts-jest": "^29.0.3",
		"ts-loader": "^9.4.1",
		"ts-node": "^10.9.1",
		"web-ext": "^7.2.0",
		"web-ext-types": "^3.2.1",
		"webextension-polyfill": "^0.10.0",
		"webpack": "^5.74.0",
		"webpack-cli": "^4.10.0"
	}
}
