{
	"version": "1.0.0",
	"name": "page-translate-robot",
	"description": "description",
	"manifest_version": 2,
	"applications": {
		"gecko": {
			"id": "{0fffffff-ffff-ffff-ffff-fffffffffff0}",
			"strict_min_version": "60.0"
		}
	},
