{
	"manifest_version": 2,
	"applications": {
		"gecko": {
			"id": "{0fffffff-ffff-ffff-ffff-fffffffffff0}",
			"strict_min_version": "60.0"
		}
	},
	"permissions": [
		"storage",
		"<all_urls>"
	],
	"icons": {
		"16": "extension-icon.svg",
		"32": "extension-icon.svg",
		"48": "extension-icon.svg",
		"96": "extension-icon.svg"
	},
	"default_locale": "ja",
	"content_scripts": [
		{
			"run_at": "document_idle",
			"matches": [
				"<all_urls>"
			],
			"js": [
				"page-content.js"
			]
		}
	],
	"options_ui": {
		"open_in_tab": true,
		"page": "application-options.html"
	}
}
